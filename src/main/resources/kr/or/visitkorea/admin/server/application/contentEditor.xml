<?xml version="1.0" encoding="UTF-8" ?>
<editor>
	<collection id="property">
	
		<group id="DEFAULT_HEADER" >
			<caption>기본 헤더</caption>
			<icon>POLYMER</icon>
			<property id="width" caption="width" icon="POLYMER" default="0px" type="STRING" />
			<property id="height" caption="height" icon="POLYMER" default="0px" type="STRING" />
			<property id="x" caption="x" icon="POLYMER" default="0px" type="STRING" />
			<property id="y" caption="y" icon="POLYMER" default="0px" type="STRING" />
		</group>
	
		<group id="SIZE" >
			<caption>크기</caption>
			<icon>POLYMER</icon>
			<property id="width" caption="width" icon="POLYMER" default="0px" type="STRING" />
			<property id="height" caption="height" icon="POLYMER" default="0px" type="STRING" />
			<property id="max-width" caption="max-width" icon="POLYMER" default="0px" type="STRING" />
			<property id="max-height" caption="max-height" icon="POLYMER" default="0px" type="STRING" />
		</group>
		
		<group id="BACKGROUND" >
			<caption>배경</caption>
			<icon>POLYMER</icon>
			<property id="background-color" caption="배경 색상" icon="POLYMER" type="COLOR" />
			<property id="background-image" caption="배경 이미지" icon="POLYMER" type="STRING" />
			<property id="background-position" caption="배경 위치" icon="POLYMER" type="STRING" />
			<property id="background-repeat" caption="배경 반복" icon="POLYMER" type="SELECT">
				<option value="repeat">repeat</option>
				<option value="repeat-x">repeat-x</option>
				<option value="repeat-y">repeat-y</option>
				<option value="no-repeat">no-repeat</option>
			</property>
		</group>
		
		<group id="TEXT" >
			<caption>텍스트 속성</caption>
			<icon>POLYMER</icon>
			<property id="color" caption="텍스트 색상" icon="POLYMER" default="#000000" type="COLOR" />
			<property id="direction" caption="텍스트 방향" icon="POLYMER" type="SELECT" >
				<option value="ltr">ltr</option>
				<option value="rtl">rtl</option>
			</property>
			<property id="line-height" caption="텍스트 줄간격" icon="POLYMER" default="150%" type="STRING" />
			<property id="letter-spacing" caption="글자 간격" icon="POLYMER" default="-0.1px"  type="SELECT" />
			<property id="text-align" caption="텍스트 정렬" icon="POLYMER" type="SELECT" >
				<option value="left">left</option>
				<option value="right">left</option>
				<option value="center">center</option>
				<option value="justify">justify</option>
			</property>
			<property id="text-decoration" caption="텍스트 장식" icon="POLYMER" type="SELECT">
				<option value="none">none</option>
				<option value="underline">underline</option>
				<option value="overline">overline</option>
				<option value="line-through">line-through</option>
				<option value="blink">blink</option>
			</property>
			<property id="text-indent" caption="들여쓰기" icon="POLYMER" default="0px"  type="STRING" />
			<property id="text-transform" caption="대소문자 지정" icon="POLYMER" type="SELECT">
				<option value="none">none</option>
				<option value="capitalize">capitalize</option>
				<option value="uppercase">uppercase</option>
				<option value="lowercase">lowercase</option>
			</property>
			<property id="word-spacing" caption="단어 간격" icon="POLYMER" default="1px" type="STRING" />
			<property id="text-weight" caption="글자 굵기" icon="POLYMER" type="SELECT" >
				<option value="normal">normal</option>
				<option value="bold">bold</option>
				<option value="bolder">bolder</option>
				<option value="lighter">lighter</option>
				<option value="number">number</option>
				<option value="initial">initial</option>
				<option value="inherit">inherit</option>
			</property>
			<property id="font-size" caption="글자 크기" icon="POLYMER" type="SELECT" >
				<option value="medium">medium</option>
				<option value="xx-small">xx-small</option>
				<option value="x-small">x-small</option>
				<option value="small">small</option>
				<option value="x-large">x-large</option>
				<option value="xx-large">xx-large</option>
				<option value="smaller">smaller</option>
				<option value="larger">larger</option>
				<option value="length">length</option>
				<option value="initial">initial</option>
				<option value="inherit">inherit</option>
			</property>
		</group>
		
