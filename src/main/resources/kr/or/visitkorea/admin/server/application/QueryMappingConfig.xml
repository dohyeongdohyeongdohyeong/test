<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="kr/or/visitkorea/admin/server/application/system.properties" />

	<settings>
		<setting name="logImpl" value="LOG4J"/>
	</settings>
	
	<typeAliases>
		<typeAlias alias="jsonObject" type="org.json.JSONObject" />
		<typeAlias alias="hashMap" type="java.util.HashMap" />
		<typeAlias alias="map" type="java.util.Map" />
		<typeAlias alias="list" type="java.util.List" />
		<typeAlias alias="int" type="java.lang.Integer" />
		<package name="kr.or.visitkorea.admin.shared.model" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
				<property name="poolPingEnabled" value="true"/>
				<property name="poolPingQuery" value="SELECT 1"/>
				<property name="poolPingConnectionsNotUsedFor" value="600000"/>
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="kr/or/visitkorea/admin/server/application/ContentMasterMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/PermissionMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/StaffMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/ArticleMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/AddressMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/TagsMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/DatabaseMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/FoodApiMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/ImageMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/DepartmentMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/siteMainMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/SnsMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/BannerMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/DepartmentInfoMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/ExcelUploadMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/AnalysisMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/NewsMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/ZikimiMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/PartnersMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/GuideBookMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/FestivalBannerMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/AppVersionMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/FcmMessageMapper.xml" />
		<mapper resource="kr/or/visitkorea/admin/server/application/RepairMapper.xml" />
